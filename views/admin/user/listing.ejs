<%- include('../partials/header'); %>

<style>
.rounded-circle {
    border-radius: 50%!important;
    height: 50px;
    width: 50px;
    object-fit: cover;
}
</style>
<!-- Main Content -->
<div class="main-content">
  <section class="section">
    <div class="section-header">
      <h1 id="animated_box"><%= currentModule; %></h1>
    </div>

    <div class="section-body">

      <div class="row">
        <div class="col-12">
          <div class="card">
            <div class="card-header">
              <h4><%= currentSubModule %></h4>
            </div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-striped table-md" id="userDataTable">
                  <thead>
                    <tr>
                      <th class="text-center">
                        #
                      </th>
                      <th>Image</th>
                      <th>User</th>
                      <th>Role</th>
                      <th>Reg. Date</th>
                      <th>Status</th>
                      <th>Verified</th>
                      <th>Action</th>
                    </tr>
                  </thead>
                  <tbody></tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

  </section>
</div>


<%- include('../partials/footer'); %>

